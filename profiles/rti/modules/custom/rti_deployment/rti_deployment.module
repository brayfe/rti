<?php

/**
 * @file
 * RTI Deployment module.
 */
function get_menu_path($link_title) {
  if ($link_title) {
    switch ($link_title) {
      case 'Featured Items':
        return 'resources/featured';

      case 'For Parents':
        return 'resources/parents';

      case 'Events':
        return 'events';

      case 'About RTI':
        return 'node/1';

      case 'Contact Us':
        return 'node/4';

      default:
        return '<front>';
    }
  }
}
